{
  "name": "@unielon/wallet-connect-react",
  "private": false,
  "version": "0.0.18",
  "type": "module",
  "author": "saul <ninasaul@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "unielon",
    "unielon wallet connect",
    "drc20",
    "dogecoin",
    "dogecoin wallet connect"
  ],
  "bugs": {
    "url": "https://github.com/unielon-org/unielon-wallet-react/issues"
  },
  "repository": {
    "url": "https://github.com/unielon-org/unielon-wallet-react",
    "type": "git"
  },
  "homepage": "https://unielon.com",
  "description": "unielon wallet js sdk rom react provider、swap pools base action, base use for React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/lib/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn run prettier",
      "prettier --write --ignore-unknown"
    ]
  },
  "files": [
    "dist",
    "index.js",
    "README.md"
  ],
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.3.0"
  }
}
